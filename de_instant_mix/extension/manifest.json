{
  "name": "De-Instant Mix Google Music",
  "version": "1.1.1",
  "description": "Google Music includes auto-generated Instant Mix playlists that cannot be removed.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["jquery-1.9.1.min.js", "de_instant_mix.js"],
      "matches": ["https://play.google.com/music/*"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  }
}