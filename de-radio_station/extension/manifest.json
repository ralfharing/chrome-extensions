{
  "name": "De-Radio Station Google Music",
  "version": "1.2.1",
  "description": "Google Music includes auto-generated Radio Station playlists that cannot be removed.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["jquery-1.9.1.min.js", "de-radio_station.js"],
      "matches": ["https://play.google.com/music/*"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  }
}